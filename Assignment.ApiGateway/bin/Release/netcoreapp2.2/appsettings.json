{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "spring": {
    "application": {
      "name": "ApiGatewayAssignment"
    }
  },
  "eureka": {
    "client": {
      "shouldRegisterWithEureka": true,
